    "import numpy as np\n",
    "#Vectorized implementation of mean centering\n",
    "y=np.array([1,2,3])\n",
    "J_n=np.ones((3,1))\n",
    "beta_hat=(J_n.T@J_n)**-1@J_n.T@y\n",
    "print(beta_hat)\n",
    "print(np.mean(y))"
   ]
  }
